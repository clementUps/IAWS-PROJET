DROP TABLE IF EXISTS FILM;
DROP TABLE IF EXISTS SALLE;
DROP TABLE IF EXISTS FILM_SALLE;

CREATE TABLE FILM(ID_FILM INT PRIMARY KEY, IMDB_ID VARCHAR(255),TITRE VARCHAR(255),ANNEE INT);
CREATE TABLE SALLE(ID_SALLE INT PRIMARY KEY, VILLE VARCHAR(255), NB_SALLES INT);
CREATE TABLE FILM_SALLE(ID INT PRIMARY KEY, ID_FILM INT, ID_SALLE INT);